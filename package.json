{
    "name": "gitlab-stats",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "npm run private:clean && concurrently -c bgBlue,bgGreen \"npm run start:client\"  \"npm run start:server\"",
        "start:client": "webpack-dev-server --watch --config ./webpack.config.client.js --mode development",
        "start:server": "webpack --watch --config ./webpack.config.server.js --mode development",
        "build:client": "webpack --watch --config ./webpack.config.client.js --mode development",
        "build:server": "webpack --config ./webpack.config.server.js --mode development",
        "build": "npm run private:clean && webpack --config ./webpack.config.js --mode production",
        "private:clean": "rimraf dist",
        "lint": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "dependencies": {
        "axios": "^0.18.0",
        "express": "^4.16.4",
        "history": "^4.7.2",
        "js-yaml": "^3.12.1",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "react-router-dom": "^4.3.1",
        "react-scripts": "2.1.3"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-react": "^7.0.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.5",
        "concurrently": "^4.1.0",
        "copy-webpack-plugin": "^4.6.0",
        "css-loader": "^2.1.0",
        "eslint": "^5.12.1",
        "eslint-plugin-react": "^7.12.4",
        "file-loader": "^3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "nodemon": "^1.18.9",
        "prop-types": "^15.6.2",
        "style-loader": "^0.23.1",
        "webpack": "^4.29.0",
        "webpack-cli": "^3.2.1",
        "webpack-dev-server": "^3.1.14",
        "webpack-node-externals": "^1.7.2",
        "webpack-shell-plugin": "^0.5.0"
    }
}
